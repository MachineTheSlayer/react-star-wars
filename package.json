{
  "name": "react-star-wars",
  "version": "0.1.0",
  "homepage": "https://machinetheslayer.github.io/react-star-wars/",
  "private": true,
  "dependencies": {
    "@storybook/storybook-deployer": "^2.8.16",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.2",
    "react-router-dom": "^7.6.2",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "storybook": "start-storybook -p 6006 -s public",
