  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/preset-create-react-app": "^9.0.16",
    "@storybook/react-webpack5": "^9.0.16",
    "eslint-plugin-storybook": "^9.0.16",
